{
  "name": "serviceprovider",
  "version": "0.0.1",
  "description": "API for accessing DBCs services through sockets and HTTP",
  "main": "src/main.js",
  "scripts": {
    "preinstall": "cp -n context-sample.json context.json",
    "apitest": "cd apitest; ./apitest.sh",
    "dev": "supervisor -w src,node_modules src/main.js",
    "dev:remoteprofile": "PROFILE_SERVICE_URL=\"http://profile-i01.dbc.dk:3001/\" npm run dev",
    "lint": "eslint ./src",
    "lint:checkstyle": "eslint ./src  -o report.xml -f checkstyle",
    "siege": "siege -c500 -d10 -t30M -i -f siegetestendpoints.txt",
    "siege:live": "siege -c50 -d10 -t10M -i -f siegedemoendpoints.txt",
    "start": "node src/main.js",
    "swagger:validate": "curl localhost:8080/v0/swagger.json | swagger validate",
    "test": "LOG_LEVEL=off mocha --compilers js:babel/register --recursive src/**/__tests__/*",
    "test:single": "LOG_LEVEL=off mocha --compilers js:babel/register",
    "tail": "tail -f log.log",
    "tail:timer": "tail -f log.log | awk '(/event/ || /time_delta/ || /TIMER/) && !/result/'",
    "transformer-proto": "node src/transformer-proto.js",
    "gtrans": "node src/genericTransformerExample.js",
    "coverage": "istanbul cover _mocha --report lcovonly --report cobertura --report html -- --compilers js:babel/register src/**/__tests__/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DBCDK/serviceprovider.git"
  },
  "keywords": [
    "serviceprovider",
    "API",
    "dbc"
  ],
  "author": "dbcdk",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DBCDK/serviceprovider/issues"
  },
  "homepage": "https://github.com/DBCDK/serviceprovider#readme",
  "dependencies": {
    "babel": "5.8.29",
    "body-parser": "1.14.1",
    "compression": "1.6.1",
    "cache-manager": "^1.0.0",
    "dbc-node-basesoap-client": "^1.1.3",
    "dbc-node-logger": "^1.4.9",
    "express": "4.13.4",
    "helmet": "0.14.0",
    "helmet-csp": "0.3.0",
    "js-yaml": "^3.5.3",
    "jsonpath-object-transform": "https://github.com/vibjerg/jsonpath-object-transform/archive/1.0.5.tar.gz",
    "lodash": "3.10.1",
    "minimist": "^1.2.0",
    "node-rest-client": "^1.5.1",
    "path": "^0.12.7",
    "request": "^2.60.0",
    "sanitize-html": "^1.11.2",
    "socketcluster": "^4.1.0",
    "supervisor": "^0.9.1",
    "swagger": "^0.7.5",
    "swagger-ui": "^2.1.4",
    "walk": "^2.3.9",
    "xml2js": "^0.4.10",
    "xmldom": "^0.1.19",
    "xpath": "0.0.9",
    "xss-filters": "^1.2.6"
  },
  "devDependencies": {
    "babel-eslint": "^4.0.5",
    "chai": "^3.2.0",
    "codeclimate-test-reporter": "^0.1.1",
    "coveralls": "^2.11.4",
    "eslint": "^1.0.0",
    "eslint-loader": "^1.0.0",
    "isparta-loader": "^1.0.0",
    "istanbul": "^0.3.22",
    "mocha": "^2.2.4",
    "sinon": "^1.17.2",
    "socketcluster-client": "^4.3.9"
  }
}
